    "@angular/core": "~10.1.6",
    "@angular/forms": "~10.1.6",
    "@angular/platform-browser": "~10.1.6",
    "@angular/platform-browser-dynamic": "~10.1.6",
    "@angular/router": "~10.1.6",
    "rxjs": "~6.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1001.7",
    "@angular-devkit/build-ng-packagr": "~0.1001.7",
    "@angular/cli": "~10.1.6",
    "@angular/compiler-cli": "~10.1.6",
    "@angular/language-service": "~10.1.6",
    "@types/jest": "^26.0.15",
    "@types/node": "~12.20.13",
    "codelyzer": "^6.0.2",
    "hammerjs": "^2.0.8",
    "jest": "^26.6.3",
    "ng-packagr": "^10.1.2",
    "ts-node": "~8.10.1",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.2",
    "typescript": "~3.9.10"
  }
